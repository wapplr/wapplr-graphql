{
    "name": "wapplr-graphql",
    "description": "GraphQl extension for Wapplr",
    "license": "MIT",
    "dependencies": {
        "wapplr": "^1.0.0",
        "graphql": "^16.10.0",
        "graphql-http": "^1.22.4",
        "graphql-compose": "^9.1.0",
        "graphql-compose-mongoose": "^10.0.0",
        "gql-query-builder": "^3.5.5"
    },
    "devDependencies": {
        "wapplr-cli": "^1.0.0"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "browserslist": [
        ">1%",
        "last 4 versions",
        "Firefox ESR",
        "not ie < 9"
