    "scripts": {
        "wapplr-graphql start": "wapplr-cli start",
        "wapplr-graphql build": "wapplr-cli build",
        "wapplr-graphql clean": "wapplr-cli clean"
    },
    "version": "1.0.74",
    "files": [
        "dist/*"
    ],
    "main": "dist/server",
    "browser": "dist/client",
    "repository": {
        "type": "git",
        "url": "https://github.com/wapplr/wapplr-graphql.git"
    }
}
