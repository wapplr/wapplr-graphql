{
    "name": "wapplr-graphql",
    "description": "GraphQl extension for Wapplr",
    "license": "MIT",
    "dependencies": {
        "wapplr": "^1.0.0",
        "graphql": "^15.4.0",
        "express-graphql": "^0.11.0",
        "graphql-compose": "^7.22.1",
        "graphql-compose-mongoose": "^9.1.0",
        "gql-query-builder": "^3.5.5"
    },
    "devDependencies": {
        "wapplr-cli": "^1.0.0"
    },
    "engines": {
        "node": ">=12.0.0"
